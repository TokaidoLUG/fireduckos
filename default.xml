<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote  name="yocto" fetch="git://git.yoctoproject.org/" pushurl="git://git.yoctoproject.org/" />
	<remote  name="openembedded" fetch="git://git.openembedded.org/" pushurl="git://git.openembedded.org/" />
	<remote  name="OSSystems" fetch="git://github.com/OSSystems/" pushurl="git://github.com/OSSystems/" />
	<remote  name="linux-sunxi" fetch="git://github.com/linux-sunxi/" pushurl="git://github.com/linux-sunxi/" />

	<remote  name="tokaidolug" fetch="https://github.com/TokaidoLUG/" pushurl="https://github.com/TokaidoLUG/" />
	<remote  name="watatuki" fetch="https://github.com/watatuki/" pushurl="https://github.com/watatuki/" />
	<remote  name="shimada" fetch="https://github.com/dejiko/" pushurl="https://github.com/dejiko/" />
	
	<default revision="master" />

	<!-- Yocto Layers -->
	<project path="poky" name="poky" remote="yocto" revision="pyro" />

	<!-- Openembedded Layers -->
	<project path="meta-openembedded" name="meta-openembedded" remote="openembedded" revision="pyro" />
	<project path="meta-browser" name="meta-browser" remote="OSSystems" revision="master" />

	<project path="meta-efl-rebuilding" name="meta-efl-rebuilding" remote="watatuki" revision="master" />

	<!-- distro Layers -->
	<project path="meta-fireduck" name="meta-fireduck" remote="tokaidolug" revision="master" />

	<!-- bsp Layers -->
	<project path="meta-jetson-tk1" name="meta-jetson-tk1" remote="watatuki" revision="pyro" />
	<project path="meta-intel" name="meta-intel" remote="yocto" revision="pyro" />
	<project path="meta-intel-mobile" name="meta-intel-mobile" remote="tokaidolug" revision="master" />
	<project path="meta-raspberrypi" name="meta-raspberrypi" remote="yocto" revision="pyro" />
	<project path="meta-sunxi" name="meta-sunxi" remote="linux-sunxi" revision="pyro" />
	<project path="meta-coconport" name="meta-coconport" remote="shimada" revision="master" />
</manifest>
